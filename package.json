{
  "name": "GenoDataUploader",
  "productName": "GenoDataUploader",
  "version": "0.0.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "babel app -d dist && gulp && electron .",
    "build": "babel app -d dist && gulp && npm run pack",
    "pack": "electron-packager . GenoDataUploader --out=release --prune --asar --version=1.3.2 --platform=darwin --arch=x64 --asar.unpack='*.node'  --ignore=node_modules/.bin --ignore=.git --ignore='node_modules/electron-*' —overwrite",
    "packwin": "electron-packager . GenoDataUploader --out=release --prune --asar --version=1.3.2 --platform=win32 --arch=ia32 --asar.unpack='*.node'  --ignore=node_modules/.bin --ignore=.git --ignore='node_modules/electron-*' —overwrite",
    "postinstall": "npm run rebuild-sqlite3",
    "rebuild-sqlite3": "bash tools/rebuild-sqlite3.sh",
    "rebuild-sqlite3-win": "tools/rebuild-sqlite3.bat"
  },
  "dependencies": {
    "ali-oss": "",
    "body-parser": "~1.15.1",
    "chokidar": "^1.6.0",
    "co": "*",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-session": "^1.14.0",
    "jade": "^1.11.0",
    "jpeg-lossless-decoder-js": "^1.2.11",
    "lodash": "^4.13.1",
    "log4js": "^0.6.37",
    "loglevel": "^1.4.1",
    "morgan": "~1.7.0",
    "q": "^1.4.1",
    "request": "^2.74.0",
    "sequelize": "^3.23.6",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.1.4",
    "walk": "^2.3.9",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-register": "^6.11.6",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-inject": "^4.1.0",
    "gulp-order": "^1.1.1",
    "gulp-replace": "^0.5.4",
    "nodemon": "^1.10.0"
  }
}
